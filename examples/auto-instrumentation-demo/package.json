{
  "name": "flowscope-auto-instrumentation-demo",
  "version": "1.0.0",
  "description": "FlowScope Auto-Instrumentation Integration Examples",
  "main": "index.js",
  "scripts": {
    "demo:auto-basic": "node examples/auto-instrumentation/basic.js",
    "demo:auto-advanced": "node examples/auto-instrumentation/advanced.js",
    "demo:import-basic": "node examples/import-replacement/basic.js", 
    "demo:import-advanced": "node examples/import-replacement/advanced.js",
    "demo:manual-decorator": "node examples/enhanced-manual/decorator.js",
    "demo:manual-context": "node examples/enhanced-manual/context.js",
    "demo:comparison": "node comparison-demo.js",
    "demo:all": "npm run demo:auto-basic && npm run demo:import-basic && npm run demo:manual-decorator && npm run demo:comparison",
    "test": "node test-runner.js",
    "test:auto-instrumentation": "node test-runner.js --filter=auto-instrumentation",
    "test:import-replacement": "node test-runner.js --filter=import-replacement", 
    "test:enhanced-manual": "node test-runner.js --filter=enhanced-manual",
    "test:comparison": "node test-runner.js --filter=comparison"
  },
  "keywords": [
    "flowscope",
    "auto-instrumentation", 
    "observability",
    "llm",
    "langchain",
    "tracing"
  ],
  "author": "FlowScope Team",
  "license": "MIT",
  "dependencies": {
    "langchain": "^0.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
