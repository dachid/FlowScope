# Environment variables for the Customer Support Bot
# Copy this file to .env and fill in your actual API keys

# OpenAI API Key for LangChain
OPENAI_API_KEY=your_openai_api_key_here

# FlowScope API Configuration
FLOWSCOPE_API_KEY=your_flowscope_api_key_here
FLOWSCOPE_SERVER_URL=http://localhost:3001

# Application Configuration
NODE_ENV=development
PORT=3003

# Database Configuration (for production)
# DATABASE_URL=postgresql://username:password@localhost:5432/customer_support

# Example API Keys for Testing (DO NOT USE IN PRODUCTION)
# OPENAI_API_KEY=sk-test123...
# FLOWSCOPE_API_KEY=fs-test123...
